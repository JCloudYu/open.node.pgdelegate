{
	"name": "pgdelegate",
	"version": "0.5.0",
	"description": "A library provides easy use of PostgresSQL library",
	"main": "_dist/pgdelegate.js",
	"scripts": {
		"prebuild": "rm -rf _dist",
		"build": "tsc",
		"postbuild": "copyfiles -e \"./node_modules/**/*\" -e \"./_dist/**/*\" \"./**/*.js\" \"./**/*.d.ts\" ./_dist"
	},
	"keywords": [
		"Postgre",
		"PostgreSQL",
		"Postgres"
	],
	"author": "JCloudYu",
	"license": "ISC",
	"dependencies": {
		"@types/pg-format": "^1.0.2",
		"pg": "^8.7.3",
		"pg-format-fix": "^1.0.5"
	},
	"devDependencies": {
		"@types/pg": "^8.6.5",
		"copyfiles": "^2.4.1"
	}
}
